//Calculate sine using cordic;
// with n iterations;
uint32_t sine(uint32_t angle, int8_t n = 5){
  int32_t x = 3535815245;          //0.6074
  int32_t y = 0;           //0
  int32_t z;
  int32_t new_x, new_y;

  if (angle>>30 == 1) { z = angle - 1<<30; }
  else if (angle>>30 == 2) { z = angle + 1<<30; }
  z = angle;
  
  for (int i = 0; i < n; i++) {
    if (z < 0) {
       new_x = x + (y >> i);
       new_y = y - (x >> i);
       z = z + tangent[i];
     }else{
       new_x = x - (y >> i);
       new_y = y + (x >> i);
       z = z - tangent[i];
     }
     x = new_x;
     y = new_y;
  }
  return y;
}



void msgInTask(void *pvParameters) {
  const TickType_t xFrequency = 5/portTICK_PERIOD_MS;
  TickType_t xLastWakeTime = xTaskGetTickCount();
  uint32_t counter = 0;
  char inMsg[] = "xxx";
  while (1) {
    while (Serial.available() > 0) {
      char tmp = Serial.read();
      uint32_t newRemoteMatrix = __atomic_load_n(&remoteMatrix, __ATOMIC_RELAXED);
      if (tmp != '\n'){
        inMsg[counter] = tmp;
        counter++;
      }else{
        counter = 0;
        if (inMsg[0] == 'R') {
          uint8_t key = 0;
          if (inMsg[2] == 'B') key = 11;
          else if (inMsg[2] == 'A') key = 10;
          else key = inMsg[2] - '0';
          newRemoteMatrix = newRemoteMatrix | (1 << key);
        }else if(inMsg[0] == 'P') {
          uint8_t key = 0;
          __atomic_store_n(&remoteOctave, inMsg[1] - '0', __ATOMIC_RELAXED);
          if (inMsg[2] == 'B') key = 11;
          else if (inMsg[2] == 'A') key = 10;
          else key = inMsg[2] - '0';
          newRemoteMatrix =  (newRemoteMatrix & (~(1 << key))); //bits 16:12 decode octave, 11:0 key
        }
        __atomic_store_n(&remoteMatrix, newRemoteMatrix , __ATOMIC_RELAXED);
      }
    }
  }
}


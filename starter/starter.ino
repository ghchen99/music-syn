   while(1) {
    vTaskDelayUntil(&xLastWakeTime, xFrequency);
     //Update display
    u8g2.clearBuffer();
    u8g2.setFont(u8g2_font_profont10_tr);
    uint32_t tempMatrix = __atomic_load_n(&matrix, __ATOMIC_RELAXED);
    uint8_t tempKnob0 = __atomic_load_n(&knob0, __ATOMIC_RELAXED);
    uint8_t tempKnob1 = __atomic_load_n(&knob1, __ATOMIC_RELAXED);
    uint8_t tempKnob3 = __atomic_load_n(&knob3, __ATOMIC_RELAXED);
    for (int i=0; i<12; i++) {
      if (!((tempMatrix>>i)&1)) u8g2.drawStr(7+i*10,10,notes[i].c_str());
    }
    u8g2.drawStr(2,20,"INS"); 
    u8g2.setCursor(7,30);
    u8g2.print(tempKnob1/2, DEC);
    u8g2.drawStr(38,20,"OCT"); 
    u8g2.setCursor(44,30);
    u8g2.print(tempKnob0, DEC);
    u8g2.drawStr(78,20,"VOL"); 
    u8g2.setCursor(80,30);
    u8g2.print(tempKnob3, DEC);
    u8g2.sendBuffer(); // transfer internal memory to the display
  }
}

void detectKnobRotation(uint8_t knob, uint8_t previous, uint8_t current){
    int8_t shift = 0;
    if (previous == 0 && current == 1) shift = 1;
    else if (previous == 0 && current == 2) shift = -1;
    else if (previous == 1 && current == 0) shift = -1;
    else if (previous == 1 && current == 3) shift = 1;
    else if (previous == 2 && current == 0) shift = 1;
    else if (previous == 2 && current == 3) shift = -1;
    else if (previous == 3 && current == 1) shift = -1;
    else if (previous == 3 && current == 1) shift = 1;
    if (knob == 1) {
      int8_t temp = __atomic_load_n(&knob1, __ATOMIC_RELAXED) + shift;
      if (temp > 4) temp = 4; if (temp < 0) temp = 0;
      __atomic_store_n(&knob1, temp, __ATOMIC_RELAXED);
    }else if (knob == 3) {
      int8_t temp = __atomic_load_n(&knob3, __ATOMIC_RELAXED) + shift;
      if (temp > 16) temp = 16; if (temp < 0) temp = 0;
      __atomic_store_n(&knob3, temp, __ATOMIC_RELAXED);
    }else if (knob == 0) {
      int8_t temp = __atomic_load_n(&knob0, __ATOMIC_RELAXED) + shift;
      if (temp > 8) temp = 8; if (temp < 0) temp = 0;
      __atomic_store_n(&knob0, temp, __ATOMIC_RELAXED);
    }
  }

void scanKeysTask(void *pvParameters){
  const TickType_t xFrequency = 20/portTICK_PERIOD_MS;
  TickType_t xLastWakeTime = xTaskGetTickCount();
  Serial.println("scanning");
  uint32_t startTime = micros();
  while (1) {
    char noteMessage[] = "xxx";
    vTaskDelayUntil(&xLastWakeTime, xFrequency);
    uint16_t currentPressed = 0;
    uint8_t current0, previous0, current1, previous1, current3, previous3 = 0;
    uint32_t newMatrix = 0;
    for (int i = 0; i < 8; i++) {
      setRow(i);
      delayMicroseconds(3);
      uint8_t row_i = readCols();
      newMatrix = newMatrix | (row_i << i*4);
    }
    
    uint32_t oldMatrix = __atomic_load_n(&matrix, __ATOMIC_RELAXED);
    previous0 = (oldMatrix >> 16) & 3; current0 =  (newMatrix >> 16) & 3;
    previous3 = (oldMatrix >> 14) & 3; current3 = (newMatrix >> 14) & 3;
    previous1 = (oldMatrix >> 18) & 3; current1 = (newMatrix >> 18) & 3; 

    bool keyChange = false;
    //atomic load here
    uint8_t tempKnob0 = __atomic_load_n(&knob0, __ATOMIC_RELAXED);
    for (int i = 0; i<12; i++){
      if (((oldMatrix >> i)&1) && !((newMatrix >> i)&1)) {
        keyChange = true;
        noteMessage[0] = 'P';
        //noteMessage[1] =knob0 + '0';
        noteMessage[1] = tempKnob0 + '0';
        noteMessage[2] = intToHex[i];
        xQueueSend( msgOutQ, (char*) noteMessage, portMAX_DELAY);  
      }else if(!((oldMatrix >> i)&1) && ((newMatrix >> i)&1)) {
        keyChange = true;
        noteMessage[0] = 'R';
        //noteMessage[1] = knob0 + '0';
        noteMessage[1] = tempKnob0 + '0';
        noteMessage[2] = intToHex[i];
        xQueueSend( msgOutQ, (char*) noteMessage, portMAX_DELAY);  
      }
    }
    oldMatrix = newMatrix;
    detectKnobRotation(0, previous0, current0);
    detectKnobRotation(1, previous1, current1);
    detectKnobRotation(3, previous3, current3);
      
    __atomic_store_n(&matrix, newMatrix, __ATOMIC_RELAXED);
  }
}

